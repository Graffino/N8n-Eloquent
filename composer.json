{
    "name": "n8n/eloquent",
    "description": "Laravel package for integrating Eloquent models with n8n workflows",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "n8n",
            "email": "info@n8n.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/events": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "N8n\\Eloquent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "N8n\\Eloquent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "N8n\\Eloquent\\Providers\\N8nEloquentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 