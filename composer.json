{
    "name": "shortdotinc/n8n-eloquent",
    "version": "2.1.2",
    "description": "A Laravel package for seamlessly integrating Eloquent models with n8n workflows.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Ciolpan",
            "email": "nick@short.inc"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/events": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Shortinc\\N8nEloquent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shortinc\\N8nEloquent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shortinc\\N8nEloquent\\Providers\\ShortincN8nEloquentServiceProvider"
            ]
        }
    },
    "keywords": [
        "laravel",
        "n8n",
        "eloquent",
        "webhooks",
        "automation",
        "workflow",
        "shortinc"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
} 